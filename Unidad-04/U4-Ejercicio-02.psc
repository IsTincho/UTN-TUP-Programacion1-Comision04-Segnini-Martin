Proceso Ejercicio_02
	
	Definir num,i como real;
	Dimension num[5];
	
	Para i<-0 hasta 4
		Escribir "Ingrese el numero: ", i+1;
		Leer num[i];
	FinPara
	
	Escribir "La lista de los numeros queda de la siguiente manera: ";
	
	Para i<-0 hasta 4
		Escribir Sin Saltar num[i], " ";
	FinPara
	
FinProceso
